<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <!-- please don't change the application name -->
    <application>development</application>

    <version>manual2</version>
    <threadsafe>true</threadsafe>
    <warmup-requests-enabled>true</warmup-requests-enabled>

    <automatic-scaling>
        <min-num-instances>1</min-num-instances>
        <max-num-instances>5</max-num-instances>
        <cool-down-period-sec>60</cool-down-period-sec>
        <cpu-utilization>
            <target-utilization>0.5</target-utilization>
        </cpu-utilization>
    </automatic-scaling>



    <!--<automatic-scaling>-->
        <!--<min-idle-instances>4</min-idle-instances>-->
        <!--<max-idle-instances>automatic</max-idle-instances>-->
        <!--<min-pending-latency>automatic</min-pending-latency>-->
        <!--<max-pending-latency>automatic</max-pending-latency>-->
    <!--</automatic-scaling>-->

    <vm>true</vm>

</appengine-web-app>